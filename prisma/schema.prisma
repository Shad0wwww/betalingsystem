
generator client {
  provider = "prisma-client"
  output = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  
}

model User {
  id       Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  phone     String   @unique
  createdAt DateTime @default(now())

  stripeCustomerId String?  @unique

  //Relationer
  
  
}

model VerificationToken {
  identifier String
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
}